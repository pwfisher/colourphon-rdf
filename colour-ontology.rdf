  </rdf:Description>
  <rdf:Description rdf:about="http://data.colourphon.co.uk/def/colour-ontology#ColourScheme">
    <rdfs:label>Colour Scheme</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.colourphon.co.uk/def/colour-ontology#ColourScheme">
    <rdfs:comment>Complementary colours together make a colour scheme.</rdfs:comment>
