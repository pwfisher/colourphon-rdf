<rdf:RDF xmlns:colour="http://data.colourphon.co.uk/def/colour-ontology#" xmlns:foaf="http://xmlns.com/foaf/0.1/" xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
  <rdf:Description rdf:about="http://data.colourphon.co.uk/def/colour-ontology#">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ontology"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.colourphon.co.uk/def/colour-ontology#">
    <rdfs:label>Colour Ontology</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.colourphon.co.uk/def/colour-ontology#">
    <rdfs:comment>Used to describe colours and their relationship to each other</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.colourphon.co.uk/def/colour-ontology#Colour">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.colourphon.co.uk/def/colour-ontology#Colour">
    <rdfs:label>Colour</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.colourphon.co.uk/def/colour-ontology#Colour">
    <rdf:comment>A thing which is a colour in some precise or abstract sense.</rdf:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.colourphon.co.uk/def/colour-ontology#Colour">
    <rdfs:isDefinedBy rdf:resource="http://data.colourphon.co.uk/def/colour-ontology#"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.colourphon.co.uk/def/colour-ontology#NamedColour">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
